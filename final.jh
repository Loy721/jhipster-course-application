entity File {
    topic String
    content String
}

entity MyTest {
    name String
}

entity Question {
    content String
}

entity MyUserMyTest {
    grade Integer
}

entity MyUser {
    name String
    surname String
}

relationship OneToMany {
    File{myTests} to MyTest{file}
    MyTest{questions} to Question{myTest}
    MyUser{myTests} to MyUserMyTest{myUser}
    MyTest{myUsers} to MyUserMyTest{myTest}
}
