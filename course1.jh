entity File {
    topic String,
    content String
}

entity MyTest {
    name String
    questions String
    file File
}

entity Question {
    content String
}

entity MyUser {
    name String
    surname String
}

entity MyUserMyTest {
    grade String
}

relationship OneToMany {
    File{myTests} to MyTest{file},
    MyTest{questions} to Question
}